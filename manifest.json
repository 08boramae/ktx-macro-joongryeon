{
  "manifest_version": 2,
  "name": "KTX Macro",
  "description": "KTX 자동 예약 매크로",
  "version": "1.0.0",
  "permissions": [
    "http://www.letskorail.com/"
  ],
  "icons": {
    "96": "icon96.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "matches": [
        "http://www.letskorail.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "page": "background.html"
  },
  "web_accessible_resources": [
    "inject.js",
    "assets/tada.mp3"
  ]
}